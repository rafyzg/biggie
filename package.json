    "express": "^4.17.1",
    "express-validator": "^6.2.0",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.4.0",
    "mocha": "^6.2.2",
    "pg": "^7.11.0",
